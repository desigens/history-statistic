{
    "manifest_version": 2,
    "version": "1.0.0",
    "minimum_chrome_version": "17",
    "name": "History statistics",
    "permissions": ["history"],
    "icons": {
        "128": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}
